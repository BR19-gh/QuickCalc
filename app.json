{
  "expo": {
    "name": "QuickCalc",
    "slug": "QuickCalc",
    "version": "1.0.15",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "locales": {
      "en": "src/lang/en.json",
      "ar": "src/lang/ar.json"
    },
    "ios": {
      "buildNumber": "2.1.15",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": ["en", "ar"],
        "CFBundleDevelopmentRegion": "en",
        "ExpoLocalization_supportsRTL": true
      },
      "bundleIdentifier": "me.br19.QuickCalc"
    },
    "extra": {
      "eas": {
        "projectId": "0dcd864d-7de0-4db7-ba1d-50790c12a613"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-quick-actions",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ]
  }
}
